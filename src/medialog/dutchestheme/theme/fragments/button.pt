<section class="${view/data/effect|None}" id="fragment-${python: view.id|None}"
         tal:define="link view/one_link|None; link_url link/getURL|None">
       <div class="roundbutton">
           <div class="button btn btn-primary ${view/data/color1|None}">
             <a class="${view/data/color2}" href="${link_url|None}">${view/data/buttontext}</a>
           </div>
       </div>
       <tal:define tal:define="load_css python:context.portal_registry['medialog.dutchestheme.interfaces.IMedialogDutchesThemeSettings.load_css'] or '';">
      	 		<link rel="stylesheet" href="++theme++dutchestheme/css/button.css" tal:condition="load_css">
     	</tal:define>
</section>
